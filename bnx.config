# platform/hardware config
CONFIG_TARGET_ar71xx=y
CONFIG_TARGET_ar71xx_generic=y
CONFIG_TARGET_ar71xx_generic_DEVICE_oolitebox=y
CONFIG_IMAGEOPT=y

# model/version config
CONFIG_VERSIONOPT=y
CONFIG_VERSION_DIST="OpenWrt"
CONFIG_VERSION_PRODUCT="BNX-2000"
CONFIG_VERSION_HWREV="1"
CONFIG_VERSION_NUMBER="localdev"
CONFIG_VERSION_REPO="/"
CONFIG_VERSION_FILENAMES=n
CONFIG_VERSION_CODE_FILENAMES=n

# package config
CONFIG_PACKAGE_bnx-base=y
CONFIG_PACKAGE_bnx-addons=y

# Defines the deployment environment.  Defaults to developer.
CONFIG_BNX_PROVISION_API_ENDPOINT="https://api.bnxcloud-dev.com"
CONFIG_SIGNED_PACKAGES=n

#
# VPN
#
# CONFIG_PACKAGE_openvpn-easy-rsa is not set
# CONFIG_PACKAGE_openvpn-mbedtls is not set
# CONFIG_PACKAGE_openvpn-nossl is not set
# CONFIG_PACKAGE_openvpn-openssl is not set
CONFIG_PACKAGE_strongswan=y

#
# Configuration
#
CONFIG_STRONGSWAN_ROUTING_TABLE="220"
CONFIG_STRONGSWAN_ROUTING_TABLE_PRIO="220"

